                endIcon={<img src={GTLicon} alt=\"GeoPin\"></img>}\n                        >\n                            <Typography variant=\"caption\" style={{ fontSize: '9px' }}>\n                                Guess The Location\n                            </Typography>\n                        </Button>\n                    }\n                    direction=\"top\"\n                    overflow=\"hidden\"\n                >\n                    <MenuHeader color=\"primary\">\n                        <Typography variant=\"caption\" style={{ color: '#f56920' }}>\n                            Pick a place\n                        </Typography>\n                    </MenuHeader>\n                    <MenuItem styles={{ background: '#1b1b1b', color: '#fafafa', borderRadius: '20px' }}>\n                        <Button\n                            style={{\n                                // marginLeft: '3px',\n                                // marginRight: '3px',\n                                textAlign: 'center',\n                                fontSize: '10px',\n                                marginBottom: '5px',\n                                background: '#fafafa',\n                                width: '90%',\n                                borderRadius: '20px',\n                                margin: 'auto',\n                            }}\n                            onClick={handleClickRightAns}\n                        >\n                            {props.correctLocation}\n                        </Button>\n                    </MenuItem>\n                    <MenuItem styles={{ background: '#1b1b1b', color: '#fafafa', borderRadius: '20px' }}>\n                        <Button\n                            style={{\n                                // marginLeft: '3px',\n                                // marginRight: '3px',\n                                textAlign: 'center',\n                                fontSize: '10px',\n                    